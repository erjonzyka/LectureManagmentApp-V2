@using LectureAppLibrary.Models
@model List<Classroom>



<table class="table table-hover align-items-center text-center">
    <thead>
        <tr>
            <th scope="col">Numri i Salles</th>
            <th scope="col">Kapaciteti</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.SallaNo</td>
                <td>@item.Kapaciteti</td>

            </tr>
        }
    </tbody>
</table>
